{
  "version": 2,
  "name": "got-quotes",
  "alias": [
    "got-quotes"
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    {
      "src": "/service-worker.js",
      "headers": {
        "cache-control": "s-maxage=0"
      }
    },
    {
      "handle": "filesystem"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ],
  "files": [
    "dist"
  ]
}
